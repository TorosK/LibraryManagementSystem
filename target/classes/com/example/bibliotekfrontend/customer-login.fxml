<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bibliotekfrontend.ControllerCustomerLogin">
   <children>
      <Label alignment="CENTER" layoutX="174.0" layoutY="112.0" text="Customer Login">
         <font>
            <Font size="36.0" />
         </font></Label>
      <Label alignment="CENTER" layoutX="251.0" layoutY="197.0" text="YYYYMMDDXXXX">
         <font>
            <Font name="System Italic" size="11.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="226.0" layoutY="223.0" promptText=" " />
      <PasswordField fx:id="passwordTextField" layoutX="226.0" layoutY="263.0" promptText="PIN" />
      <Button fx:id="loginButton" layoutX="279.0" layoutY="297.0" mnemonicParsing="false" onAction="#cSendLoginDataToBackEnd" text="Login">
         <font>
            <Font name="System Italic" size="12.0" />
         </font></Button>
      <Button fx:id="backToFrontPageButton" layoutX="14.0" layoutY="511.0" mnemonicParsing="false" onAction="#cBackToFirstPage" text="Back to front page">
         <font>
            <Font name="System Italic" size="12.0" />
         </font></Button>
      <Button fx:id="toRegisterButton" layoutX="270.0" layoutY="414.0" mnemonicParsing="false" onAction="#cToRegisterPage" text="Register!">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Button>
      <Label layoutX="178.0" layoutY="384.0" text="Click Register if you want to become a member!">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Label fx:id="errorLabelLoginCustomer" alignment="CENTER" layoutX="148.0" layoutY="165.0" prefHeight="25.0" prefWidth="309.0" textAlignment="CENTER" />
   </children>
</AnchorPane>
