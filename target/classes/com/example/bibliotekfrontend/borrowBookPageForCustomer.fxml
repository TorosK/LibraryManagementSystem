<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="600.0" style="-fx-background-color: #c39351;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bibliotekfrontend.ControllerBorrowBookPageForCustomer">
   <children>
      <ListView fx:id="borrowBookListView" layoutX="19.0" layoutY="90.0" onMouseClicked="#cListViewForBorrow" prefHeight="398.0" prefWidth="564.0" />
      <TextField fx:id="searchBorrowBookTextField" layoutX="161.0" layoutY="60.0" onAction="#cSearchForBorrowBook" promptText="Sök.." />
      <Label layoutX="266.0" layoutY="14.0" text="Låna bok">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Button fx:id="searchBorrowBookButton" layoutX="335.0" layoutY="60.0" mnemonicParsing="false" onAction="#cSearchForBorrowBook" text="Sök Bok" />
      <Button layoutX="552.0" layoutY="513.0" mnemonicParsing="false" onAction="#cBorrowBook" text="Låna" />
      <Button fx:id="backToCustomerFirstPageButton" layoutX="14.0" layoutY="513.0" mnemonicParsing="false" onAction="#backToCustomerFirstPage" text="Tillbaka" />
      <Label fx:id="selectedBookForBorrow" alignment="CENTER_RIGHT" layoutX="115.0" layoutY="517.0" prefHeight="17.0" prefWidth="428.0" />
      <Label fx:id="borrowErrorConfirmation" alignment="CENTER_RIGHT" layoutX="115.0" layoutY="492.0" prefHeight="17.0" prefWidth="475.0" />
   </children>
</AnchorPane>
